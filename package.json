{
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "prebuild": "pnpm db:push && pnpm db:seed",
    "build": "next build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbopack",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "simple-git-hooks",
    "start": "next start"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "latest",
    "@libsql/client": "latest",
    "@radix-ui/react-alert-dialog": "latest",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-slot": "latest",
    "@t3-oss/env-nextjs": "latest",
    "bcrypt": "latest",
    "class-variance-authority": "latest",
    "clsx": "latest",
    "drizzle-orm": "latest",
    "jotai": "latest",
    "jotai-dark": "latest",
    "jotai-effect": "latest",
    "lucide-react": "latest",
    "next": "latest",
    "next-auth": "beta",
    "nuqs": "latest",
    "react": "latest",
    "react-dom": "latest",
    "recharts": "latest",
    "sonner": "latest",
    "tailwind-merge": "latest",
    "tailwindcss-animate": "latest",
    "usehooks-ts": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "latest",
    "@iconify-json/mingcute": "latest",
    "@iconify-json/simple-icons": "latest",
    "@types/bcrypt": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "dotenv": "latest",
    "drizzle-kit": "latest",
    "eslint": "latest",
    "eslint-config-hyoban": "latest",
    "lint-staged": "latest",
    "postcss": "latest",
    "simple-git-hooks": "latest",
    "tailwindcss": "latest",
    "tsx": "latest",
    "typescript": "latest"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
